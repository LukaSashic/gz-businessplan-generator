{
  "name": "gz-businessplan-generator",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered business plan generator for German GrÃ¼ndungszuschuss applications",
  "author": "Sasa",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:types": "supabase gen types typescript --local > src/types/database.ts",
    "compliance:financial": "ast-grep --lang typescript -p '$A + $B' lib/finance && ast-grep --lang typescript -p '$A * $B' lib/finance",
    "compliance:dsgvo": "ast-grep --lang typescript -p 'anthropic.messages.$$$' --not -p 'anthropic-beta'",
    "compliance:rls": "ast-grep --lang typescript -p '.from($TABLE)' --not -p 'user_id'",
    "compliance:a11y": "ast-grep --lang tsx -p '<button $$$>' --not -p 'aria-label'",
    "compliance:all": "npm run compliance:financial && npm run compliance:dsgvo && npm run compliance:rls && npm run compliance:a11y",
    "prepare": "husky install"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/ssr": "^0.8.0",
    "@tailwindcss/typography": "^0.5.19",
    "ai": "^3.4.33",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "decimal.js": "^10.4.3",
    "highlight.js": "^11.11.1",
    "idb-keyval": "^6.2.2",
    "jotai": "^2.16.2",
    "jotai-devtools": "^0.13.0",
    "lucide-react": "^0.460.0",
    "next": "^15.1.0",
    "nuqs": "^2.0.0",
    "partial-json": "^0.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@ast-grep/cli": "^0.30.0",
    "@axe-core/playwright": "^4.10.0",
    "@playwright/test": "^1.48.0",
    "@sentry/nextjs": "^8.0.0",
    "@supabase/supabase-js": "^2.90.1",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/ui": "^2.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.1.0",
    "husky": "^9.0.0",
    "langfuse": "^3.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.0",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "supabase": "^1.200.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
